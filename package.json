{
  "name": "react-dux",
  "version": "1.0.0",
  "description": "react & redux",
  "main": "index.js",
  "scripts": {
    "dll": "webpack --config ./build/webpack/webpack.config.dll.js --progress --profile --colors",
    "build": "npm run dll && npm run pack",
    "pack": "webpack --config ./build/webpack/webpack.config.production.js --progress --profile --colors",
    "hot-server": "cross-env NODE_ENV=local node --trace-warnings -r babel-register ./node_modules/webpack-dev-server/bin/webpack-dev-server --config ./build/webpack/webpack.config.development.js --progress --profile --colors",
    "dev": "webpack --config ./build/webpack/webpack.config.development.js && webpack-dev-server --config ./build/webpack/webpack.config.development.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rxfe/react-dux.git"
  },
  "keywords": [
    "react"
  ],
  "author": "lanmingming@meituan.com",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/rxfe/react-dux/issues"
  },
  "homepage": "https://github.com/rxfe/react-dux#readme",
